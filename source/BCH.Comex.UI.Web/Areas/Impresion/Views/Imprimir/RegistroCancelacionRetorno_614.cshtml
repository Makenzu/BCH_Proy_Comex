<body>
    <div id="page_1">


        <div id="id_1">
            <div id="id_1_1">
                <p class="p0 ft0">@Model.TituloCabezal</p>
            </div>
             <div id="id_1_2">
                <p class="p0 ft3">@Model.Ciudad</p>
                <p class="p2 ft3"><nobr>@Model.NuestraReferencia&nbsp;&nbsp;&nbsp;&nbsp;</nobr><nobr><span class="p2 ft3">@Model.NuestraReferenciaValue</span></nobr></p>
                @if (Model.SuReferenciaValue != null)
                {
                    <p class="p2 ft3"><nobr>@Model.SuReferencia</nobr><nobr><span class="p2 ft3">@Model.SuReferenciaValue</span></nobr></p>
                }
                <p  class="p3 ft3"><nobr>Monto @Model.MonedaCapital&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</nobr><nobr><span class="p3 ft3">@Model.MontoCapital</span></nobr></p>

            </div>

        </div>
        <div id="id_2">
            <p class="p0 ft2">Señor(es)</p>
            <p class="p0 ft2">@Model.NombreCliente</p>
            <p class="p0 ft2">@Model.SubDireccion1</p>
            <p class="p1 ft3">@Model.CiudadCliente</p>
            <p class="p2 ft1" style="white-space: pre;word-wrap: break-word;">@Model.Parrafo1</p>
            <table cellpadding=0 cellspacing=0 class="t0">
                <tr>
                    <td class="tr0 td0"><p class="p3 ft4">Beneficiario</p></td>
                    <td class="tr0 td1"><p class="p4 ft4">Vía de la Distribución</p></td>
                    <td class="tr0 td2"><p class="p3 ft4">Concepto</p></td>
                    <td class="tr0 td3"><p class="p5 ft4">Monto</p></td>
                </tr>
                @foreach (var item in Model.LineasRemesa)
                {
                    <tr>
                        <td class="tr1 td0"><p class="p3 ft1">@item.Beneficiario</p></td>
                        <td class="tr1 td1"><p class="p3 ft1">@item.ViaRemesa</p></td>
                        <td class="tr1 td2"><p class="p6 ft1">@item.Concepto</p></td>
                        <td class="tr1 td3"><p class="p7 ft1">@item.Monto</p></td>
                    </tr>
                }
            </table>
            <p class="p8 ft1">De acuerdo a sus instrucciones, hemos procedido a emitir planillas de ingreso de comercio visible, según el siguiente detalle:</p>
            <table cellpadding=0 cellspacing=0 class="t1">
                <tr style="height: 0px">
                    <td class="tr1 td4"></td>
                    <td class="tr1 td5"></td>
                    <td class="tr1 td6"></td>
                    <td class="tr1 td7"></td>
                    <td class="tr1 td9"></td>
                    <td class="tr1 td10"></td>
                </tr>
                <tr>
                    <td class="tr0 td"><p class="p3 ft4">Planilla</p></td>
                    <td class="tr0 td"><p class="p9 ft4">Código</p></td>
                    <td class="tr0 td"><p class="p10 ft4">Declaración</p></td>
                    <td class="tr0 td"><p class="p3 ft5">&nbsp;</p></td>
                    <td colspan=2 class="tr0 td"><p class="p5 ft4">Monto</p></td>
                </tr>
                @foreach (var item in Model.LineasComercioInvisible)
                {
                    <tr>
                        <td class="tr1 td"><p class="p3 ft1">@item.CodigoPlanilla</p></td>
                        <td class="tr1 td"><p class="p11 ft1">@item.Codigo</p></td>
                        <td class="tr1 td"><p class="p12 ft1"><nobr>@item.Declaracion</nobr></p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td colspan=2 class="tr1 td"><p class="p7 ft1">@item.Monto</p></td>
                    </tr>
                }


                @if (Model.LineasComisionGasto.Count > 0)
                {
                    <tr>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p13 ft4">Abonos y/o Cargos</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                    </tr>
                    <tr>
                        <td colspan=3 class="tr1 td"><p class="p3 ft1">En consecuencia efectuamos los abonos y/o débitos que se detallan:</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                    </tr>
                }

                @foreach (var item in Model.LineasComisionGasto.Where(i => i.Tipo == "D").ToList())
                {
                    <tr>
                        <td colspan=3 class="tr1 td"><p class="p3 ft1">@item.TipoOperacion</p></td>
                        <td class="tr1 td"><p class="p14 ft1">@item.Moneda</p></td>
                        <td class="tr1 td"><p class="p7 ft1">@item.Monto</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                    </tr>
                }

                @if (Model.LineasComisionGasto.Where(i => i.Tipo == "C").ToList().Count > 0)
                {
                    <tr>
                        <td colspan=2 class="tr1 td"><p class="p3 ft6">Detalle comisiones y gastos cobrados:</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                    </tr>
                }

                @foreach (var item in Model.LineasComisionGasto.Where(i => i.Tipo == "C").ToList())
                {
                    <tr>
                        <td class="tr1 td"><p class="p3 ft1">@item.TipoOperacion</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr1 td"><p class="p14 ft1">@item.Moneda</p></td>
                        <td class="tr1 td"><p class="p7 ft1">@item.Monto</p></td>
                        <td class="tr1 td"><p class="p3 ft5">&nbsp;</p></td>
                    </tr>
                }

                @if (Model.LineasComisionGasto.Where(i => i.Tipo == "C").ToList().Count > 0)
                {
                    <tr>
                        <td class="tr0 td"><p class="p3 ft4">&nbsp;</p></td>
                        <td colspan=5 class="tr3 td"><p class="p15 ft8">________________________________</p></td>
                    </tr>
                }

                @if (Model.LineasComisionGasto.Where(i => i.Tipo == "C").ToList().Count > 0)
                {
                    <tr>
                        <td class="tr3 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr3 td"><p class="p3 ft5">&nbsp;</p></td>
                        <td class="tr3 td"><p class="p15 ft8">Total</p></td>
                        <td class="tr3 td"><p class="p14 ft1">$</p></td>
                        <td class="tr3 td"><p class="p7 ft1">@Model.TotalComisionGasto</p></td>
                        <td class="tr3 td"><p class="p3 ft5">&nbsp;</p></td>
                    </tr>
                }
            </table>
            <p class="p16 ft1" style="white-space: pre-wrap;word-wrap: break-word;">@Model.PiePagina</p>
            @*<p class="p17 ft1">¡¡CONSULTE SUS OPERACIONES DE NEGOCIOS INTERNACIONALES EN INTERNET!! CONTÁCTENOS EN www.internacional.bancochile.cl</p>*@
        </div>
        <div id="id_3">
            <p class="p0 ft2">_______________________</p>
            <p class="p18 ft3">Banco de Chile</p>
        </div>
    </div>

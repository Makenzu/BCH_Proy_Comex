<body>
    <div id="page_1">
        <div id="id_1">
            <div id="id_1_1">
                <p class="p0 ft0">Servicios M/E</p>
            </div>
            <div id="id_1_2">
                <p class="p0 ft3">@Model.Ciudad</p>
                <p class="p2 ft3"><nobr>@Model.NuestraReferencia</nobr><nobr><span class="ft1">@Model.NuestraReferenciaValue</span></nobr></p>
                @if (Model.SuReferenciaValue != null)
                {
                  <p class="p2 ft3"><nobr>@Model.SuReferencia</nobr><nobr><span class="ft1">@Model.SuReferenciaValue</span></nobr></p>
                }
            </div>
        </div>
        <div id="id_1">
            <div id="id_1_3">
                <p class="p1 ft1">Señores:</p>
                <p class="p0 ft1">@Model.NombreCliente</p>
                <p class="p2 ft2">@Model.DireccionCliente</p>
                <p class="p2 ft2">@Model.CiudadCliente</p>
            </div>
        </div>
        <div id="id_2">
            <p class="p0 ft3">@Model.Parrafo1</p>
            <table cellpadding="0" cellspacing="0" class="t0" >
                <tr>
                    <td style="text-align: center; width: 650px;">
                        <p class="p4 ft5">@Model.TituloPlanillaImport:</p>
                    </td>
                </tr>
            </table>
            <table cellpadding=0 cellspacing=0 class="t0" >
                <tr>
                    <td class="tr1 td0"><p class="p3 ft4">&nbsp;</p></td>
                    <td class="tr1 td1"><p class="p5 ft6">Monto</p></td>
                     @if (Model.LineasTransferencia[0].TipoCambio!=null)
                     {   
                        <td class="tr1 td2"><p class="p6 ft3">Tipo Cambio</p></td>
                     }
                    <td class="tr1 td3"><p class="p3 ft3">Nº Planilla</p></td>
                    <td class="tr1 td15"><p class="p12 ft3">Declaración de Importación</p></td>
                </tr>
                @foreach (var item in Model.LineasTransferencia)
                {
                    <tr>
                        <td class="tr1 td0"><p class="p3 ft3">@item.Moneda</p></td>
                        <td class="tr1 td1"><p class="p7 ft3">@item.Monto</p></td>
                        @if (item.TipoCambio != null) { 
                           <td class="tr1 td2"><p class="p8 ft3">@item.TipoCambio</p></td>
                        }
                        <td class="tr1 td3"><p class="p3 ft3">@item.NroPlanilla</p></td>
                        <td class="tr1 td15"><p class="p12 ft3">@item.DeclaracionImportacion</p></td>
                    </tr>
                }
            </table>
            @if (Model.LineasRemesa.Count > 0)
            {
                <p class="p9 ft2">La moneda extranjera vendida fue distribuida de la siguiente forma :</p>
                <table cellpadding=0 cellspacing=0 class="t1" >
                    <tr>
                        <td class="tr0 td4"><p class="p10 ft5">Beneficiario</p></td>
                        <td class="tr0 td5"><p class="p10 ft5">Vía de la Remesa</p></td>
                        <td class="tr0 td6"><p class="p3 ft4">&nbsp;</p></td>
                        <td class="tr0 td7"><p class="p11 ft5">Monto</p></td>
                    </tr>
                    @foreach (var item in Model.LineasRemesa)
                    {
                        <tr>
                            <td class="tr1 td4"><p class="p10 ft3">@item.Beneficiario</p></td>
                            <td class="tr1 td5"><p class="p10 ft3">@item.ViaRemesa</p></td>
                            <td class="tr1 td6"><p class="p3 ft3">@item.Moneda</p></td>
                            <td class="tr1 td7"><p class="p12 ft3">@item.Monto</p></td>
                        </tr>
                    }
                </table>
            }
            @if (Model.LineasCargoAbono.Count > 0)
            {
                <table cellpadding="0" cellspacing="0"  class="t0">
                    <tr>
                        <td style="width: 650px; text-align: center;">
                            <p class="p4 ft5">Cargos y/o Abonos</p>
                        </td>
                    </tr>
                </table>
                
                <p class="p14 ft3">En consecuencia efectuamos los débitos y/o abonos que se detallan:</p>
                <table cellpadding=0 cellspacing=0 class="t2" >
                    @foreach (var item in Model.LineasCargoAbono)
                    {
                        <tr>
                            <td class="tr0 td8"><p class="p10 ft3">@item.NombreVia</p></td>
                            <td class="tr0 td5"><p class="p10 ft3">@item.DescripcionVia</td>
                            <td class="tr0 td6"><p class="p3 ft3">@item.Moneda</p></td>
                            <td class="tr0 td7"><p class="p12 ft3">@item.Monto</p></td>
                        </tr>
                    }
                </table>
            }
            @if (Model.LineasImpuesto.Count > 0)
            {
                <p class="p16 ft5">Detalle :</p>
                <table cellpadding=0 cellspacing=0 class="t3" >
                    @foreach (var item in Model.LineasImpuesto)
                    {
                        <tr>
                            <td class="tr2 td16"><p class="p22 ft3">@item.Detalle</p></td>
                            <td class="tr2 td17"><p class="p17 ft3">@item.Moneda</p></td>
                            <td class="tr2 td18"><p class="p12 ft3">@item.Monto</p></td>
                        </tr>
                    }
                    <tr class="tr4">
                        <td class="td16">&nbsp;</td>
                        <td colspan="2" class="td19" style="border-bottom: 1px solid black;">&nbsp;</td>
                    </tr>
                    <tr>
                        <td class="tr0 td16"><p class="p22 ft3">&nbsp;</p></td>
                        <td class="tr0 td17"><p class="p17 ft3">Total : $</p></td>
                        <td class="tr0 td18"><p class="p12 ft3">@Model.TotalImpuestos</p></td>
                    </tr>
                </table>
            }
            
            <p class="p19 ft3" style="white-space: pre-wrap;word-wrap: break-word;">@Model.PiePagina</p>
        </div>
        <div id="id_3">
            <p class="p0 ft8">_______________________</p>
            <p class="p21 ft10">BANCO <span class="ft9">DE </span>CHILE</p>
        </div>
    </div>

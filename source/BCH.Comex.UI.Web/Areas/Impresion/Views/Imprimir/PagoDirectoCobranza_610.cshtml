@model BCH.Comex.Core.Entities.Cext01.DocumentoReporteModel
@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=8">
    <title>Pago directo cobranza</title>
    <style type="text/css">
        body {
            margin-top: 0px;
            margin-left: 0px;
        }

        #page_1 {
            position: relative;
            overflow: hidden;
            margin: 60px 0px 187px 76px;
            padding: 0px;
            border: none;
            width: 740px;
        }

            #page_1 #id_1 {
                border: none;
                margin: 0px 0px 0px 0px;
                padding: 0px;
                border: none;
                width: 740px;
                overflow: hidden;
            }

                #page_1 #id_1 #id_1_1 {
                    float: left;
                    border: none;
                    margin: 1px 0px 0px 0px;
                    padding: 0px;
                    border: none;
                    width: 390px;
                    overflow: hidden;
                }

                #page_1 #id_1 #id_1_2 {
                    float: left;
                    border: none;
                    margin: 0px 0px 0px 0px;
                    padding: 0px;
                    border: none;
                    width: 350px;
                    overflow: hidden;
                }

            #page_1 #id_2 {
                border: none;
                margin: 32px 0px 0px 0px;
                padding: 0px;
                border: none;
                width: 740px;
                overflow: hidden;
            }

            #page_1 #id_3 {
                border: none;
                margin: 296px 0px 0px 429px;
                padding: 0px;
                border: none;
                width: 311px;
                overflow: hidden;
            }





        .ft0 {
            font: bold 16px 'Times New Roman';
            line-height: 16px;
        }

        .ft1 {
            font: 13px 'Times New Roman';
            line-height: 15px;
        }

        .ft2 {
            font: bold 10px 'Times New Roman';
            line-height: 9px;
        }

        .ft3 {
            font: bold 13px 'Times New Roman';
            line-height: 9px;
        }

        .ft4 {
            font: italic bold 13px 'Times New Roman';
            line-height: 15px;
        }

        .ft5 {
            font: italic bold 13px 'Times New Roman';
            line-height: 16px;
        }

        .ft6 {
            font: italic bold 13px 'Times New Roman';
            text-decoration: underline;
            line-height: 15px;
        }

        .ft7 {
            font: 1px 'Times New Roman';
            line-height: 1px;
        }

        .ft8 {
            font: 13px 'Times New Roman';
            line-height: 14px;
        }

        .ft9 {
            font: bold 12px 'Times New Roman';
            line-height: 14px;
        }

        .p0 {
            text-align: left;
            margin-top: 0px;
            margin-bottom: 0px;
        }

        .p1 {
            text-align: left;
            padding-left: 2px;
            margin-top: 0px;
            margin-bottom: 0px;
            white-space: nowrap;
        }

        .p2 {
            text-align: right;
            margin-top: 0px;
            margin-bottom: 0px;
            white-space: nowrap;
        }

        .p3 {
            text-align: left;
            margin-top: 0px;
            margin-bottom: 0px;
            white-space: nowrap;
        }

        .p4 {
            text-align: right;
            padding-right: 58px;
            margin-top: 0px;
            margin-bottom: 0px;
            white-space: nowrap;
        }

        .p5 {
            text-align: left;
            margin-top: 1px;
            margin-bottom: 0px;
        }

        .p6 {
            text-align: left;
            padding-right: 178px;
            margin-top: 11px;
            margin-bottom: 0px;
        }

        .p7 {
            text-align: left;
            margin-top: 30px;
            margin-bottom: 0px;
        }

        .p8 {
            text-align: right;
            padding-right: 41px;
            margin-top: 0px;
            margin-bottom: 0px;
            white-space: nowrap;
        }

        .p9 {
            text-align: right;
            padding-right: 2px;
            margin-top: 0px;
            margin-bottom: 0px;
            white-space: nowrap;
        }

        .p10 {
            text-align: left;
            padding-left: 358px;
            margin-top: 0px;
            margin-bottom: 0px;
            white-space: nowrap;
        }

        .p11 {
            text-align: left;
            padding-right: 191px;
            margin-top: 59px;
            margin-bottom: 0px;
        }

        .p12 {
            text-align: left;
            padding-right: 224px;
            margin-top: 16px;
            margin-bottom: 0px;
        }

        .p13 {
            text-align: left;
            padding-left: 17px;
            margin-top: 1px;
            margin-bottom: 0px;
        }

        .td0 {
            padding: 0px;
            margin: 0px;
            width: 121px;
            vertical-align: bottom;
        }

        .td1 {
            padding: 0px;
            margin: 0px;
            width: 127px;
            vertical-align: bottom;
        }

        .td2 {
            padding: 0px;
            margin: 0px;
            width: 400px;
            vertical-align: bottom;
        }

        .td3 {
            padding: 0px;
            margin: 0px;
            width: 59px;
            vertical-align: bottom;
        }

        .td4 {
            padding: 0px;
            margin: 0px;
            width: 118px;
            vertical-align: bottom;
        }

        .td5 {
            border-bottom: #000000 1px solid;
            padding: 0px;
            margin: 0px;
            width: 118px;
            vertical-align: bottom;
        }

        .tr0 {
            height: 12px;
        }

        .tr1 {
            height: 18px;
        }

        .tr2 {
            height: 19px;
        }

        .tr3 {
            height: 29px;
        }

        .tr4 {
            height: 15px;
        }

        .tr5 {
            height: 14px;
        }

        .tr6 {
            height: 17px;
        }

        .t0 {
            width: 248px;
            font: 13px 'Times New Roman';
        }

        .t1 {
            width: 577px;
            margin-top: 12px;
            font: 13px 'Times New Roman';
        }
    </style>
</head>

<body>
    <div id="page_1">
        <div id="id_1">
            <div id="id_1_1">
                <p class="p0 ft0">@Model.TituloCabezal.ToUpper()</p>
            </div>
            <div id="id_1_2">
                <p class="p0 ft1">@Model.Ciudad</p>
                <table cellpadding=0 cellspacing=0 class="t0">
                    <tr>
                        <td class="tr0 td0"><p class="p1 ft2">@Model.NuestraReferencia</p></td>
                        <td class="tr0 td1"><p class="p2 ft3"><nobr>@Model.NuestraReferenciaValue</nobr></p></td>
                    </tr>
                    <tr>
                        <td class="tr1 td0"><p class="p3 ft1">@Model.SuReferencia</p></td>
                        <td class="tr1 td1"><p class="p4 ft1">@Model.SuReferenciaValue</p></td>
                    </tr>
                </table>
            </div>
        </div>
        <div id="id_2">
            <p class="p0 ft4">Señores</p>
            <p class="p0 ft4">@Model.NombreCliente</p>
            <p class="p0 ft4">@Model.DireccionCliente</p>
            <p class="p5 ft5">@Model.CiudadCliente</p>
            @*<p class="p5 ft5">Santiago</p>*@
            <p class="p6 ft1">Cúmplenos informarles que, con esta fecha hemos procedido a descargar de nuestros registros la cobranza indicada en la referencia, en atención a que ha sido cancelada en forma directa a ustedes.</p>
            <p class="p7 ft1">En consecuencia de lo anterior hemos efectuado los débitos y abonos que se detallan:</p>
            <table cellpadding=0 cellspacing=0 class="t1">
                @foreach (var item in Model.LineasComisionGasto.Where(i => i.Tipo == "D").ToList())
                {
                    <tr>
                        <td class="tr2 td2"><p class="p3 ft1">@item.TipoOperacion</p></td>
                        <td class="tr2 td3"><p class="p8 ft1">@item.Moneda</p></td>
                        <td class="tr2 td4"><p class="p9 ft1">@item.Monto</p></td>
                    </tr>
                }
                @*<tr>
                    <td class="tr2 td2"><p class="p3 ft1">Otro Nemónico Moneda Nacional</p></td>
                    <td class="tr2 td3"><p class="p8 ft1">$</p></td>
                    <td class="tr2 td4"><p class="p9 ft1">41.864,00</p></td>
                </tr>*@
                <tr>
                    <td class="tr3 td2"><p class="p3 ft6">Detalle comisiones y gastos cobrados:</p></td>
                    <td class="tr3 td3"><p class="p3 ft7">&nbsp;</p></td>
                    <td class="tr3 td4"><p class="p3 ft7">&nbsp;</p></td>
                </tr>
                @foreach (var item in Model.LineasComisionGasto.Where(i => i.Tipo == "C").ToList())
                {
                    <tr>
                        <td class="tr3 td2"><p class="p3 ft1">@item.TipoOperacion</p></td>
                        <td class="tr3 td3"><p class="p8 ft1">@item.Moneda</p></td>
                        <td class="tr3 td4"><p class="p9 ft1">@item.Monto</p></td>
                    </tr>
                }
                @*<tr>
                    <td class="tr3 td2"><p class="p3 ft1">Comision</p></td>
                    <td class="tr3 td3"><p class="p8 ft1">$</p></td>
                    <td class="tr3 td4"><p class="p9 ft1">35.180,00</p></td>
                </tr>
                <tr>
                    <td class="tr4 td2"><p class="p3 ft1">IVA</p></td>
                    <td class="tr4 td3"><p class="p8 ft1">$</p></td>
                    <td class="tr5 td5"><p class="p9 ft8">6.684,00</p></td>
                </tr>*@
                <tr>
                    <td class="tr6 td2"><p class="p10 ft9">TOTAL</p></td>
                    <td class="tr6 td3"><p class="p8 ft1">@Model.MonedaTotal</p></td>
                    <td class="tr6 td4"><p class="p9 ft1">@Model.TotalComisionGasto</p></td>
                </tr>
            </table>
            <p class="p11 ft1" style="white-space: pre-wrap;word-wrap: break-word;">@Model.PiePagina</p>
        </div>
        <div id="id_3">
            <p class="p0 ft4">_______________________</p>
            <p class="p13 ft5">Banco de Chile</p>
        </div>
    </div>
</body>
</html>

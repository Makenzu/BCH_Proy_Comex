                                    showRefresh: true, height: 535,
                                    clickToSelect: true,
                                    search: true,
                                    cache: false
                                }
                                  );
                                $('#tabla').bootstrapTable('insertRow', {
                                    index: 0,
                                    row: {
                                        id: UltimoIndex,
                                        rut: RutUser,
                                        dv: DvUser,
                                        nombre: NombreUser,
                                        tipo: TipoUser
                                    }
                                });

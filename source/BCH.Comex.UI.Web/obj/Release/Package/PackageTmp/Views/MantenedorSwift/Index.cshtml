<div class="panel panel-default" id="pnlResultados" hidden>
    <div class="panel-heading">
        <h2 class="panel-title" id="Titulo"></h2>
    </div>
    <div class="panel-body">
        @* Usuarios *@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaUsuario" title="Búsqueda de usuarios"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarUsuario" title="Modificar usuario"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarUsuario" title="Ingresar usuario"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarUsuario" title="Eliminar usuario"></button>
        @* Casillas *@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaCasilla" title="Búusqueda de casillas"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarCasilla" title="Modificar casilla"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarCasilla" title="Ingresar casilla"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarCasilla" title="Eliminar casilla"></button>
        @*Monedas*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaMoneda" title="Búsqueda de monedas"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarMoneda" title="Modificar moneda"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarMoneda" title="Ingresar moneda"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarMoneda" title="Eliminar moneda"></button>
        @*Bancos*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaBanco" title="Búsqueda de bancos"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarBanco" title="Modificar banco"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarBanco" title="Ingresar banco"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarBanco" title="Eliminar banco"></button>
        @*Paridades*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaParidad" title="Búsqueda de paridades"></button>
        @*<button class="btn btn-info" id="btnActualizarParidad">Modificar paridad</button>
            <button class="btn btn-info" id="btnIngresarParidad">Ingresar paridad</button>
            <button class="btn btn-info" id="btnEliminarParidad">Eliminar paridad</button>*@
        @*Valores campos*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaValoresCampos" title="Búsqueda de valores de campos"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarValoresCampos" title="Modificar valor de campo"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarValoresCampos" title="Ingresar valor de campo"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarValoresCampos" title="Eliminar valor de campo"></button>
        @*@Tipos mensajes*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaTiposMensajes" title="Búsqueda de tipos de mensajes"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarTiposMensajes" title="Modificar tipo de mensaje"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarTiposMensajes" title="Ingresar tipo de mensaje"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarTiposMensajes" title="Eliminar tipo de mensaje"></button>
        @*Formato mensajes*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaFormatoMensajes" title="Búsqueda de formato de mensajes"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarFormatoMensajes" title="Modificar formato de mensaje"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarFormatoMensajes" title="Ingresar formato de mensaje"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarFormatoMensajes" title="Eliminar formato de mensaje"></button>
        @*Campo mensajes*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaCampoMensajes" title="Busqueda de campos"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarCampoMensajes" title="Modificar campo"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarCampoMensajes" title="Ingresar campo"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarCampoMensajes" title="Eliminar campo"></button>
        @*Golsa de campos*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaGlosaCampos" title="Búsqueda de glosas"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarGlosaCampos" title="Modificar glosa"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarGlosaCampos" title="Ingresar glosa"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarGlosaCampos" title="Eliminar glosa"></button>
        @*Caracteres invalidos*@
        <button class="glyphicon glyphicon-search btn btn-transparent btn-group-lg" id="btnBusquedaCaracterInvalido" title="Búsqueda de caracteres inválidos"></button>
        <button class="glyphicon glyphicon-edit btn btn-transparent btn-group-lg" id="btnActualizarCaracterInvalido" title="Modificar caracter inválido"></button>
        <button class="glyphicon glyphicon-pencil btn btn-transparent btn-group-lg" id="btnIngresarCaracterInvalido" title="Ingresar caracter inválido"></button>
        <button class="glyphicon glyphicon-erase btn btn-transparent btn-group-lg" id="btnEliminarCaracterInvalido" title="Eliminar caracter inválido"></button>
        <table id="tabla"></table>

    </div>
</div>
<div id="modalUsuarioSwift" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="Usuario Swift">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="modalTitle">Búsqueda de Usuario Unidad Swift.</h4>
            </div>
            <div class="panel-body" id="divUsuarioSwift">
                <div id="DivElemtosBusquedaUsuarios">
                    <div class="row">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Rut:</label>
                            <div class="col-sm-6">
                                <input id="TxtRutUsuarioBusqueda" type="text" class="input-sm" size="15" maxlength="8">-
                                <input id="TxtCodRutListaBusqueda" type="text" class="input-sm upperCaseField" size="1" maxlength="1">
                                <br />
                                <span id="errmsgAgregaRutBusqueda"></span>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Nombre:</label>
                            <div class="col-sm-6">
                                <input id="txtnombreusuariobusqueda" type="text" class="input-sm upperCaseField" size="15" maxlength="40">
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Tipo:</label>
                            <div class="col-sm-6">
                                <select class="input-sm" id="selectFiltroEstadoBuscaUsuario" name="selectFiltroEstadoBuscaUsuario">
                                    <option value="T">Todos</option>
                                    <option value="J">Jefe</option>
                                    <option value="A">Administrador</option>
                                    <option value="O">Operador</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-body" id="divListaNombreUsuarios" hidden>
                    <div class="row">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Nombre:</label>
                            <div class="col-sm-7">
                                <input id="TxtNombreUsuarioLista" type="text" class="input-sm upperCaseField" size="37">
                                <button class="btn btn-info btn-sm pull-right" id="btnIngresaNombreUsuarioLista">Agregar</button>
                                <br />
                                <span id="errmsgAgregaNombreUsuario"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div class="pull-left col-sm-2">
                            </div>
                            <div class="col-lg-7">
                                <select multiple class="form-control" id="SeleccionNombreUsuarios" size="7"></select>
                                <br />
                                <button class="btn btn-info btn-sm pull-right" id="btnQuitaNombreUsuarios">Quitar</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer" id="DivBuscarUsuario">
                    <button class="btn btn-info" id="btnBusquedaUser" data-dismiss="modal" aria-hidden="true">Buscar</button>
                </div>
            </div>
        </div>
    </div>
</div>

            //Actualizar usuario.
            function ActualizaUsuario() {
                if (NombreUsuario == $('#TxtNombreUsuario').val() && TipoUsuario == $('#selectFiltroEstadoModificaUsuario').val()) {
                    $('#LblInfoErrorModificaUser').text("No se han modificado los registros del Usuario: " + rowSelected["nombre"]);
                    $('#divinfoModificaUser').show();
                    $('#divInforErrorModificaUser').show();
                    $('#DivFooterInfoModificaUser').show();
                    $('#divModificaUsuarioSwift').hide();
                    $('#DivFooterModificaUser').hide();
                    $('#divInforExitoModificaUser').hide();
                }
                else {
                    var TxtNombreUsuario = $('#TxtNombreUsuario').val();
                    TxtNombreUsuario = TxtNombreUsuario.trim();
                    if (!TxtNombreUsuario) {
                        $('#LblInfoErrorModificaUser').text("No se ha podido modificar el Usuario. El nombre es obligatorio.");
                        $('#divinfoModificaUser').show();
                        $('#divInforErrorModificaUser').show();
                        $('#DivFooterInfoModificaUser').show();
                        $('#divModificaUsuarioSwift').hide();
                        $('#DivFooterModificaUser').hide();
                        $('#divInforExitoModificaUser').hide();
                    }
                    else {
                        $.get('@Url.Action("UpdateUsuarios")', { rut: rowSelected["rut"], nombre: $('#TxtNombreUsuario').val(), tipo: $('#selectFiltroEstadoModificaUsuario').val() }, function (data) {
                            var status = data;
                            if (status == true) {
                                var mensajesSeleccionados = $('#tabla').bootstrapTable('getData');

                                for (var i = 0; i < mensajesSeleccionados.length; i++) {
                                    if (mensajesSeleccionados[i]["rut"] == rowSelected["rut"]) {

                                        $('#tabla').bootstrapTable('updateRow', {
                                            index: i,
                                            row: {

                                                nombre: $('#TxtNombreUsuario').val(),
                                                tipo: $('#selectFiltroEstadoModificaUsuario').val()
                                            }
                                        });
                                    }
                                }
                                $('#LblInfoExitoModificaUser').text("Usuario modificado correctamente");
                                NombreUsuario = $('#TxtNombreUsuario').val();
                                TipoUsuario = $('#selectFiltroEstadoModificaUsuario').val();
                                $('#divinfoModificaUser').show();
                                $('#divInforExitoModificaUser').show();
                                $('#DivFooterInfoFinalizadoModificaUser').show();
                                $('#DivFooterModificaUser').hide();
                                $('#divModificaUsuarioSwift').hide();
                                $('#DivFooterInfoModificaUser').hide();
                            }
                            else {
                                $('#LblInfoErrorModificaUser').text("No se ha podido modificar el Usuario.");
                                $('#divinfoModificaUser').show();
                                $('#divInforErrorModificaUser').show();
                                $('#DivFooterInfoModificaUser').show();
                                $('#divModificaUsuarioSwift').hide();
                                $('#DivFooterModificaUser').hide();
                                $('#divInforExitoModificaUser').hide();
                            }

                        });
                    }
                }
            };
            //Ingresar usuario.
            function IngresaUsuario() {
                var RutUser = $('#TxtRutUsuarioIngresa').val();
                var DvUser = $('#TxtCvUsuarioIngresa').val();
                var NombreUser = $('#TxtNombreUsuarioIngresa').val();
                var TipoUser = $('#selectFiltroEstadoIngresaUsuario').val();
                RutUser = RutUser.trim();
                DvUser = DvUser.trim();
                NombreUser = NombreUser.trim();
                if (RutUser.length <= 0 || DvUser.length <= 0 || NombreUser.length <= 0) {
                    $('#LblInfoErrorIngresoUser').text("Parámetros incorrectos");
                    $('#divinfoIngresoUsuario').show();
                    $('#divInforErrorIngresoUser').show();
                    $('#divIngresaUsuarioSwift').hide();
                    $('#DivFooterIngresaUsuario').hide();
                    $('#DivFooterInfoIngresaUsuario').show();
                    $('#divInforExitoIngresoUser').hide();

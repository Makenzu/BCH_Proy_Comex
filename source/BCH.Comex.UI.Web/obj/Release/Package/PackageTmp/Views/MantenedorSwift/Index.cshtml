                    else {
                        $.get('@Url.Action("InsertUsuarios")', { rut: RutUser, CvRut: DvUser, nombre: NombreUser, tipo: TipoUser }, function (data) {
                            var status = data;
                            if (status == true) {
                                $('#tabla').bootstrapTable({
                                    locale: "es-SP",
                                    uniqueId: 'id',
                                    pagination: true,
                                    pageSize: 11,
                                    pageList: [5, 10, 15, 20],

        function ValidaRut() {

            var rut = $('#TxtRutUsuarioIngresa').val();
            var secuencia = [2, 3, 4, 5, 6, 7, 2, 3];
            var sum = 0;
            for (var i = rut.length - 1; i >= 0; i--) {
                var d = rut.charAt(i)
                sum += new Number(d) * secuencia[rut.length - (i + 1)];
            };
            var rest = 11 - (sum % 11);
            return rest === 11 ? 0 : rest === 10 ? "K" : rest;
        };



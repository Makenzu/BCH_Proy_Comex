                    var statusRut = ValidaRut();
                    if (statusRut != DvUser) {
                        $('#LblInfoErrorIngresoUser').text("Digito verificador inválido.");
                        $('#divinfoIngresoUsuario').show();
                        $('#divInforErrorIngresoUser').show();

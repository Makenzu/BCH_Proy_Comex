                <br />
                <div class="form-group">
                    <div class="col-xs-12" id="prtTipoInteres">
                        @foreach (var item in Model.prtTipoInteres)
                        {
                            if (item.Selected)
                            {
                                if (item.Selected && !item.Enabled)
                                {
                                    <div class="radio">
                                        <label>
                                            @Html.RadioButtonFor(m => m.prtTipoInteres, item.ID, new Dictionary<string, object> { { "checked", "checked" }, { "disabled", "disabled" } })
                                            @item.Value
                                        </label>
                                    </div>
                                }
                                else
                                {
                                    <div class="radio">
                                        <label>
                                            @Html.RadioButtonFor(m => m.prtTipoInteres, item.ID, new Dictionary<string, object> { { "checked", "checked" } })
                                            @item.Value
                                        </label>
                                    </div>
                                }
                            }
                            else
                            {
                                if (!item.Enabled)
                                {
                                    <div class="radio">
                                        <label>
                                            @Html.RadioButtonFor(m => m.prtTipoInteres, item.ID, new Dictionary<string, object> { { "disabled", "disabled" } })
                                            @item.Value
                                        </label>
                                    </div>
                                }
                                else
                                {
                                    <div class="radio">
                                        <label>
                                            @Html.RadioButtonFor(m => m.prtTipoInteres, item.ID)
                                            @item.Value
                                        </label>
                                    </div>
                                }
                            }
                        }


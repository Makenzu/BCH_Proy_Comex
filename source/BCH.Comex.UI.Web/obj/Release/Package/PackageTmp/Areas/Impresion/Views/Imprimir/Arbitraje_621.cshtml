<body>
    <div id="page_1">
        <div id="id_1">
            <div id="id_1_1">
                <p class="p0 ft0">@Model.TituloCabezal.ToUpper()</p>
            </div>
            <div id="id_1_2">
                <p class="p0 ft1">@Model.Ciudad</p>
                <table cellpadding=0 cellspacing=0 class="t0">
                    <tr>
                        <td class="tr0 td0"><p class="p1 ft2">@Model.NuestraReferencia</p></td>
                        <td class="tr0 td1"><p class="p2_1 ft2"><nobr>@Model.NuestraReferenciaValue</nobr></p></td>
                    </tr>
                    @if (Model.SuReferenciaValue != null)
                       {
                        <tr>
                            <td class="tr0 td0"><p class="p1 ft2">@Model.SuReferencia</p></td>
                            <td class="tr0 td1"><p class="p2_1 ft2"><nobr>@Model.SuReferenciaValue</nobr></p></td>
                         </tr>
                       }
                </table>
            </div>
        </div>
        <div id="id_2">
            <p class="p0 ft3">Señores</p>
            <p class="p0 ft3">@Model.NombreCliente</p>
            <p class="p0 ft3">@Model.DireccionCliente</p>
            <p class="p0 ft4">@Model.CiudadCliente</p>
            @*<p class="p3 ft4">Santiago</p>*@
            @if (Model.LineasDivisa.Count > 0)
            {
                <p class="p4 ft1">Hemos efectuado Arbitraje de Divisas, según el siguiente detalle/distribución:</p>
                <table cellpadding=0 cellspacing=0 class="t1">
                    <tr>
                        <td colspan=2 class="tr0 td2"><p class="p5 ft5">Compra de Divisas:</p></td>
                        <td colspan=2 class ="tr0 td3"><p class="p6 ft5">Venta de Divisas:</p></td>
                        <td class="tr0 td5"><p class="p7 ft5">Paridades:</p></td>
                    </tr>
                    @foreach (var item in Model.LineasDivisa)
                    {
                        <tr>
                            <td class="tr1 td6"><p class="p1 ft1">@item.MonedaCompra</p></td>
                            <td class="tr1 td7"><p class="p8 ft1">@item.MontoCompra</p></td>
                            <td class="tr1 td3_1"><p class="p1 ft1">@item.MonedaVenta</p></td>
                            <td class="tr1 td4"><p class="p2 ft1">@item.MontoVenta</p></td>
                            <td class="tr1 td5"><p class="p2 ft1">@item.Paridades</p></td>
                        </tr>
                    }
                    @*<tr>
                        <td class="tr1 td6"><p class="p1 ft1">US$</p></td>
                        <td class="tr1 td7"><p class="p8 ft1">18.779,04</p></td>
                        <td class="tr1 td3"><p class="p1 ft1">EUR</p></td>
                        <td class="tr1 td4"><p class="p2 ft1">15.120,00</p></td>
                        <td class="tr1 td5"><p class="p2 ft1">0,805152</p></td>
                    </tr>*@
                </table>
            }
            @if (Model.LineasRemesa.Count > 0)
            {
                <p class="p9 ft1">La moneda extranjera vendida fue distribuida de la siguiente forma :</p>
                <table cellpadding=0 cellspacing=0 class="t2">
                    <tr>
                        <td class="tr0 td8"><p class="p1 ft5">Beneficiario</p></td>
                        <td class="tr0 td9"><p class="p1 ft5">Vía de la Remesa</p></td>
                        <td class="tr0 td10"><p class="p1 ft6">&nbsp;</p></td>
                        <td class="tr0 td11"><p class="p10 ft5">Monto</p></td>
                    </tr>
                    @foreach (var item in Model.LineasRemesa)
                    {
                        <tr>
                            <td class="tr2 td8"><p class="p11 ft1">@item.Beneficiario</p></td>
                            <td class="tr2 td9"><p class="p11 ft1">@item.ViaRemesa</p></td>
                            <td class="tr2 td10"><p class="p1 ft1">@item.Moneda</p></td>
                            <td class="tr2 td11"><p class="p2 ft1">@item.Monto</p></td>
                        </tr>
                    }
                    @*<tr>
                        <td class="tr2 td8"><p class="p11 ft1"><nobr>Kipco-damaco</nobr> Nv</p></td>
                        <td class="tr2 td9"><p class="p11 ft1">Orden de Pago O.Paises</p></td>
                        <td class="tr2 td10"><p class="p1 ft1">EUR</p></td>
                        <td class="tr2 td11"><p class="p2 ft1">15.120,00</p></td>
                    </tr>*@
                </table>
            }
            @if (Model.LineasCargoAbono.Count > 0)
            {
                <p class="p12 ft7">Cargos y/o Abonos</p>
                <p class="p13 ft1">En consecuencia efectuamos los débitos y/o abonos que se detallan:</p>
                <table cellpadding=0 cellspacing=0 class="t3">
                    <tr style="height:0px;">
                        <td class="tr3 td12"></td>
                        <td class="tr3 td13"></td>
                        <td class="tr3 td14"></td>
                        <td class="tr3 td14"></td>
                        <td class="tr3 td15"></td>
                        <td class="tr3 td15"></td>
                    </tr>
                    @foreach (var item in Model.LineasCargoAbono)
                    {
                        <tr>
                            <td class="tr3 td"><p class="p14 ft1">@item.DescripcionVia</p></td>
                            <td class="tr3 td"><p class="p1 ft1">@item.NombreVia</p></td>
                            <td colspan=2 class="tr3 td"><p class="p15 ft1">@item.Moneda</p></td>
                            <td colspan=2 class="tr3 td"><p class="p2 ft1">@item.Monto</p></td>
                        </tr>
                    }
                    @if (Model.LineasComisionGasto.Count > 0)
                    {
                        <tr>
                            <td colspan=2 class="tr5 td"><p class="p1 ft10">Detalle comisiones y gastos cobrados:</p></td>
                            <td class="tr5 td"><p class="p1 ft6">&nbsp;</p></td>
                            <td class="tr5 td"><p class="p1 ft6">&nbsp;</p></td>
                            <td class="tr5 td"><p class="p1 ft6">&nbsp;</p></td>
                            <td class="tr5 td"><p class="p1 ft6">&nbsp;</p></td>
                        </tr>
                        foreach (var item in Model.LineasComisionGasto)
                        {
                            <tr>
                                <td class="tr1 td"><p class="p1 ft1">@item.TipoOperacion</p></td>
                                <td class="tr1 td"><p class="p1 ft6">&nbsp;</p></td>
                                <td class="tr1 td"><p class="p16 ft1">@item.Moneda</p></td>
                                <td class="tr1 td"><p class="p1 ft6">&nbsp;</p></td>
                                <td class="tr1 td"><p class="p17 ft1">@item.Monto</p></td>
                                <td class="tr1 td"><p class="p1 ft6">&nbsp;</p></td>
                            </tr>
                        }
                        
                        <tr>
                          <!--<td class="tr0 td12"><p class="p17 ft6">&nbsp;</p></td>-->
                          <td colspan=6 class="tr3 td"><p class="p18 ft2">___________________________________</p></td>
                        </tr>
                        
                        <tr>
                            <td class="tr3 td"><p class="p1 ft6">&nbsp;</p></td>
                            <td class="tr3 td"><p class="p18 ft2">Total</p></td>
                            <td class="tr3 td"><p class="p16 ft1">$</p></td>
                            <td class="tr3 td"><p class="p1 ft6">&nbsp;</p></td>
                            <td class="tr3 td"><p class="p17 ft1">@Model.TotalComisionGasto</p></td>
                            <td class="tr3 td"><p class="p1 ft6">&nbsp;</p></td>
                        </tr>
                    }
                    
                </table>
            }
            
            <p class="p9 ft1" style="white-space: pre-wrap;word-wrap: break-word;">@Model.PiePagina</p>
        </div>
        <div id="id_3">
            <p class="p0 ft3">_______________________</p>
            <p class="p21 ft4">Banco de Chile</p>
        </div>
    </div>
</body>

                    <tr>
                        <td class="tr1 td0"><p class="p6 ft1">@item.Ordenante</p></td>
                        <td class="tr1 td1"><p class="p4 ft6">@item.Referencia</p></td>
                        <td class="tr1 td2"><p class="p4 ft1">@item.Moneda</p></td>
                        <td class="tr1 td3"><p class="p7 ft1">@item.Monto</p></td>
                    </tr>
                }

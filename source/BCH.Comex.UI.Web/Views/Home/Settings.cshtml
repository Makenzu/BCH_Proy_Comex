@{
    ViewBag.Title = "Configuracion";
    ViewBag.Favicon = @Url.Content("~/favicon.ico");
}
@section css{
    <link rel="stylesheet" href="~/Content/css/bootstrap-table.min.css">
    <style type="text/css">
        dt {
            width: 350px !important;
        }


    </style>
}

@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $("#Combo_ConfigImpres_PrintFormat").change(function () {
                $("#btnSave").show();
            });

            $("#btnSave").click(function () {
                $.post('@Url.Action("SaveSettings")',
                    { ConfigImpres_PrintFormat: $("#Combo_ConfigImpres_PrintFormat").val() },
                    function () {
                        $("#btnSave").hide();
                        window.location = '@Url.Action("Index")';
                    });
            });
        });
    </script>
    
}
